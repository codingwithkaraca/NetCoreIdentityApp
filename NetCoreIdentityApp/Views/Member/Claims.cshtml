@{
    ViewData["Title"] = "Claims";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<ClaimVM>

<h1>Claim Listesi</h1>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Id</th>
        <th>Type</th>
        <th>Value</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Issuer</td>
        <td>@item.Type</td>
        <td>@item.Value</td>
    </tr>
    }
    </tbody>
</table>

